<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml"
      fx:controller="view.gui.ProgramListController">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Label text="Select a program to execute:" style="-fx-font-size: 14px;"/>
    <ListView fx:id="programListView" VBox.vgrow="ALWAYS"/>
    <Button text="Execute" onAction="#executeProgram" prefWidth="100"/>
</VBox> 